# Investment Management System - Frontend Production Environment
# Note: Only variables prefixed with VITE_ are exposed to the client

# API Configuration - Production
VITE_API_URL=https://api.yourdomain.com/api
VITE_API_TIMEOUT=30000

# WebSocket Configuration - Production
VITE_WS_URL=wss://api.yourdomain.com:9001

# Application Configuration
VITE_APP_NAME=Investment Management System
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Feature Flags - Production
VITE_USE_MOCK_API=false
VITE_ENABLE_DEBUG=false
VITE_FEATURE_2FA=true
VITE_FEATURE_REAL_TIME_UPDATES=true
VITE_FEATURE_NOTIFICATIONS=true
VITE_FEATURE_ANALYTICS=true

# Security Configuration
VITE_ENABLE_HTTPS=true
VITE_SECURE_COOKIES=true

# Performance Configuration
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true
VITE_CACHE_STRATEGY=cache-first

# Build Configuration
VITE_BUILD_SOURCEMAP=false
VITE_BUILD_MINIFY=true
