{
  "name": "magic-patterns-vite-template",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "start": "npm run start:prod",
    "start:dev": "npm run dev",
    "start:prod": "npm run build && npm run preview",
    "build": "npx vite build",
    "build:prod": "NODE_ENV=production npx vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "preview": "npx vite preview --host 0.0.0.0 --port 5173",
    "backend": "npm --prefix backend start",
    "backend:dev": "npm --prefix backend run dev",
    "backend:prod": "npm --prefix backend run start:prod",
    "start:full": "npm-run-all --parallel backend:prod start:prod",
    "start:full:dev": "npm-run-all --parallel backend:dev dev",
    "test:production": "node backend/test-production-fixes.js",
    "optimize:db": "node backend/optimize-database-production.js",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "npm run clean && rm -rf node_modules backend/node_modules"
  },
  "dependencies": {
    "@types/mongoose": "^5.11.96",
    "@types/uuid": "^10.0.0",
    "axios": "^1.10.0",
    "date-fns": "^4.1.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.441.0",
    "mongoose": "^8.16.0",
    "nodemailer": "^7.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "recharts": "^2.12.7",
    "recharts-to-png": "^2.4.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.18",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "latest",
    "eslint": "^8.50.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "npm-run-all": "^4.1.5",
    "postcss": "latest",
    "tailwindcss": "3.4.17",
    "typescript": "^5.5.4",
    "vite": "^5.2.0"
  }
}
